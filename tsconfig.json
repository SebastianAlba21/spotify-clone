{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@/components": ["src/components/*"],
			"@/icons": ["src/icons/*"],
			"@/layouts": ["src/layouts/*"],
			"@/lib": ["src/lib/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
